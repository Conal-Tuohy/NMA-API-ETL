				<crm:E42_Identifier rdf:about="#referencenumber">
					<rdf:value>1234.5678.0001</rdf:value>
					<crm:P2_has_type rdf:resource="http://vocab.getty.edu/aat/300312355" />
				</crm:E42_Identifier>
			</crm:P1_is_identified_by>
		</x:expect>
	</x:scenario>

	<x:scenario label="Convert title">
